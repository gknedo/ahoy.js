{
  "name": "ahoy.js",
  "version": "0.3.1",
  "homepage": "https://github.com/ankane/ahoy.js",
  "description": "Simple, powerful JavaScript analytics",
  "main": "dist/ahoy.js",
  "module": "dist/ahoy.module.js",
  "jsnext:main": "dist/ahoy.module.js",
  "scripts": {
    "build": "yarn build:module && yarn build:umd && yarn build:umd:min",
    "build:module": "cross-env NODE_ENV=es rollup -c -o dist/ahoy.module.js",
    "build:umd": "cross-env NODE_ENV=development rollup -c -o dist/ahoy.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c -o dist/ahoy.min.js",
    "lint": "eslint src",
    "test": "zuul test/ahoy_test.js",
    "test:local": "zuul --local 8080 test/ahoy_test.js"
  },
  "dependencies": {
    "object-to-formdata": "^1.3.0"
  },
  "keywords": [
    "analytics",
    "locations",
    "referral"
  ],
  "authors": [
    "ankane"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ankane/ahoy.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.18.1",
    "faux-jax": "^5.0.6",
    "js-cookie": "^2.1.3",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "tape": "^4.6.3",
    "zuul": "^3.11.1"
  }
}
